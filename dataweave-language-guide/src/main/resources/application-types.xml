        <types:message>
          <types:payload type="outputType"/>
        </types:message>
      </types:input-event>
      <types:output-event>
        <types:message>
          <types:payload type="outputType"/>
        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
  <types:enrichment select="#7bd527af-a8a7-4bf0-997d-2c84b2cc12d7">
