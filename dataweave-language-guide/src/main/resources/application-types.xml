<?xml version='1.0' encoding='UTF-8'?>
<types:mule xmlns:types="http://www.mulesoft.org/schema/mule/types">
  <types:catalog>
    <types:type name="outputType" format="json">
      <types:shape format="weave" example="output.json"><![CDATA[%dw 2.0
type outputType = {|
  title: String, 
  author: String, 
  year: Number {"typeId": "int"}
|} {"MetadataFormat": "json",
"example": "{\"title\":\"Java 8 in Action\",\"author\":\"Mario Fusco\",\"year\":2014}"}

]]></types:shape>
    </types:type>
    <types:type name="timestamp" format="java">
      <types:shape format="raml"><![CDATA[#%RAML 1.0 DataType
type: datetime]]></types:shape>
    </types:type>
  </types:catalog>
  <types:enrichment select="#f1421fce-0145-4d9e-82d5-8be788f2f9db">
